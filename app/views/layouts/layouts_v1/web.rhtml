<?xml version="1.0" encoding="iso-8859-1"?><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN""http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xml:lang="en" lang="en" xmlns="http://www.w3.org/1999/xhtml"><head><title>Layout 3 - <%= @page_title %></title><%= stylesheet_link_tag "layout_3.css" %><%= javascript_include_tag "prototype" %><%= javascript_include_tag "swfobject" %><% if @include_tinymce_script -%>  <%= javascript_include_tag "tiny_mce/tiny_mce.js" %>  <%= javascript_include_tag "tiny_mce_config" %><% end -%><!--meta data start--><meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" /><meta name="robots" content="NOODP"><% if @metatitle%><meta name="title" content="<%= @metatitle %>" /><% end %><% if @metakeywords%><meta name="keywords" content="<%= @metakeywords %>" /><% end %><% if @metadescription%><meta name="description" lang="en" content="<%= @metadescription %>" /><%end%><!--meta data end--></head><body><div id="maincontainer"><p class="skip"><a href="#content" accesskey="s">Skip to the main content</a></p>                <div id="top">                <%= link_to image_tag("../images/layout_3/logo.gif", :alt => " ", :title => " ", :border => "0", :hspace => "0", :vspace => "0", :class => "logo")+ '', {:controller => 'web', :action => 'index'}, {:accesskey => "0"} %>                        <div><%= link_to(image_tag("../images/layout_3/icon_email.gif", :alt => "Email", :title => "Email", :border => "0", :hspace => "0", :vspace => "0") + "<br /> email", :controller => "web", :action => "contact") %></div>                        <div><%= link_to(image_tag("../images/layout_3/icon_home.gif", :alt => "Home", :title => "Home", :border => "0", :hspace => "0", :vspace => "0") + "<br /> home", :controller => "web", :action => "index") %></div>                </div>                <div class="content_container">                        <div id="side">                                <div id="navigation">                                        <%= link_to "Page 1", {:controller => "web", :action => "index"}, {:accesskey => "2"} %><%= link_to "Page 2", {:controller => "web", :action => "index"}, {:accesskey => "3"} %><%= link_to "Page 3", {:controller => "web", :action => "index"}, {:accesskey => "4"} %><%= link_to "Page 4", {:controller => "web", :action => "index"}, {:accesskey => "5"} %><%= link_to "News Stories", {:controller => "news"}, {:accesskey => "6"} %><%= link_to "Contact us", {:controller => "web", :action => "contact"}, {:accesskey => "7"} %>                                </div>                                <div>                                        <h2>Latest News</h2>                                                <div>                                                        <%= link_to "News Headline One", :action=>"#" %><br />                                                        <%= image_tag "/images/layout_3/pic_news_1.jpg", :alt=>" ", :title=>" ", :width=>"61", :height=>"61", :border=>"0" %>                                                        <span class="font_grey">[ dd.mm.yyyy ]</span><br />                                                        Lorem ipsum iriure maiorum gubergren ut vis, ut nostro ancillae vis gubergren...                                                        <hr />                                                        <%= link_to "News Headline Two", :action=>"#" %><br />                                                        <%= image_tag "/images/layout_3/pic_news_2.jpg", :alt=>" ", :title=>" ", :width=>"61", :height=>"61", :border=>"0" %>                                                        <span class="font_grey">[ dd.mm.yyyy ]</span><br />                                                        Lorem ipsum iriure maiorum gubergren ut vis, ut nostro ancillae vis gubergren...                                        </div>                                </div>                        </div>                        <div id="content">                                <%= @content_for_layout %>                        </div>                        <div id="footer">                                        <%= link_to "&copy; Your Company Name Here 2008", :action=>"index" %> | <a href="http://www.tmedia.co.uk" target="_blank">Web Design By T Media Ltd</a> | <%= link_to "Accessibility", {:controller => "web", :action => "accessibility"}, {:accesskey => "1"} %> | <a href="http://validator.w3.org/check?uri=referer" target="_blank" class="footer">Valid XHTML</a> | <a href="http://jigsaw.w3.org/css-validator/check/referer" target="_blank" class="footer">Valid CSS</a>                        </div>                </div></div></body></html>