<h1><%= @page_title = "View All #{@dm_one['plural'].capitalize}" -%></h1><% form_tag({}, :method => "GET") do %><p>  <%= text_field_tag(:search, (session[@list_name] || {})[:search]) %>  <br />  <%= submit_tag 'Search', :class => 'submit' %> &nbsp; <%= link_to "Clear", :search => "" %></p><% end %><% if @article_pages.count != 0 -%>  <%= admin_table({:columns =>[:name, :last_updated],    :list => @articles,    :pages => @article_pages  })%><% else -%>  There are currently no <%= @dm_one['plural'].capitalize %> in the system.<% end -%>