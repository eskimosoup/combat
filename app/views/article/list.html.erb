<% @page_title = @dm_one['plural'].capitalize %><h1><%= @page_title %></h1><%= render :partial => 'navigation' %><div id="news"><% if @list_name -%>  <h2><%= @list_name%></h2><% else -%>  <h2><%= month_name(session[:month].to_i) + " - " + params[:year].to_s %></h2><% end -%>  <% if @articles.length != 0 -%>  <% for article in @articles -%>    <hr />    <%= render( :partial => 'summary',                      :locals => {:article => article}) %>  <% end -%>    <hr />  <% else -%>  <p><strong>There are currently no <%= @dm_one['plural'].downcase %> to be displayed.</strong></p>  <% end -%>  <% if @article_pages.count != 0 %>  <div class="pagecount">    <% if @article_pages.previous_page -%>      <%= link_to '&lt;&lt; Back', :action => 'list', :page => @article_pages.previous_page %>    <% end -%>    <%= @article_pages.range %> of <%= @article_pages.count %>    <% if @article_pages.next_page -%>      <%= link_to 'Next &gt;&gt;', :action => 'list', :page => @article_pages.next_page %>    <% end -%>  </div><% end %></div>